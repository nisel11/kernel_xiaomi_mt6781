/*stereo dtsi config*/
&i2c6 {
/* AWINIC AW87XXX Smart K PA */
	aw87xxx_pa@5B {
		compatible = "awinic,aw87xxx_pa";
		reg = <0x5B>;
		reset-gpio = <&pio 164 0>;
		dev_index = < 0 >;
		status = "okay";
	};

	aw87xxx_pa@58 {
		compatible = "awinic,aw87xxx_pa";
		reg = <0x58>;
		reset-gpio = <&pio 165 0>;
		dev_index = < 1 >;
		status = "okay";
	};
/* AWINIC AW87XXX Smart K PA End */
};
